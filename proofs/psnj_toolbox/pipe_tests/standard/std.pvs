std : THEORY
BEGIN
  P, Q: bool
  simple_imp : LEMMA P IMPLIES P
  modus_ponens : LEMMA (P IMPLIES Q) IMPLIES P IMPLIES Q
  nat : TYPE+
  zero : nat
  even : [nat -> bool]
  fa_hyp : LEMMA (FORALL (n: nat): P IMPLIES (even(n))) IMPLIES P IMPLIES P
  fa_instantiate : LEMMA (FORALL (n: nat) : P IMPLIES even(n)) IMPLIES P IMPLIES even(zero)
END std
